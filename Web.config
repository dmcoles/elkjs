<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.0"/>
  </system.web>
  <system.webServer>
    <modules runAllManagedModulesForAllRequests="true"/>
    <staticContent>
      <mimeMap fileExtension=".rom" mimeType="application/octet-stream" />
      <mimeMap fileExtension=".uef" mimeType="application/octet-stream" />
    </staticContent>
  </system.webServer>
</configuration>